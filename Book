
public class Book{
	
	private int isbn;
	private String title, author, category, status;
	
	public Book(int id, String t, String a, String c, String s){
		this.isbn = id;
		this.title = t;
		this.author = a;
		this.category = c;
		this.status = s;
	}
	
	public String toString(){
		return "ISBN ID: \"" + isbn + "\", Title: \"" + title +
				"\", Author: \"" + author + "\", Category: \"" + category + "\", Status: \"" + status + "\"";
	}

}
